#include <iostream>

using namespace std;

int main()
{
  int p, q, n;

  cin >> p;
  cin >> q;
  cin >> n;
  int arr[n - 1];
  for (int i = 0; i < n; i++)
    cin >> arr[i];

  for (int i = 0; i < n; i++)
  {
    if (arr[i] != p)
    {
      if (i = n)
      {
        arr[i] = arr[0];
      }
      else
        arr[i] = arr[i + 1];
    }

    cout << arr[i];
  }

  //  for(int i=0;i<n;i++) cout<<arr[i];
}
